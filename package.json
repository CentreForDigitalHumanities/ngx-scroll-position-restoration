{
  "name": "ngx-scroll-position-restoration",
  "version": "0.1.5",
  "scripts": {
    "dev--ngx-scroll-position-restoration": "ng build ngx-scroll-position-restoration --watch",
    "dev--demo-app": "ng serve demo-app --open",
    "build-demo-app": "rm -rf ./dist/  && ng build ngx-scroll-position-restoration --configuration=production && ng build demo-app --configuration=production",
    "deploy-demo-app-on-github-pages": "npm run build-demo-app &&  cp ./dist/demo-app/index.html ./dist/demo-app/404.html && gh-pages -d ./dist/demo-app/ &&  rm -rf ./dist/",
    "build": "rm -rf ./dist/ && ng build ngx-scroll-position-restoration --configuration=production",
    "publish-library": "npm run build && npm publish ./dist/ngx-scroll-position-restoration/ &&  rm -rf ./dist/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@medv/finder": "^2.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
